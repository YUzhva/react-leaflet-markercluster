{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "arrow-parens": ["error", "always"],
    "no-console": [1, { "allow": ["warn"] }],

    "react/jsx-filename-extension": 0,

    "react/forbid-prop-types": 0,
    "no-underscore-dangle": ["error", { "allow": ["_container"] }]
  }
}
